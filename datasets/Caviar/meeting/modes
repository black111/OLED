%modeh(initiatedAt(fighting(+person,+person),+time))
%modeh(terminatedAt(fighting(+person,+person),+time))

%modeh(initiatedAt(moving(+person,+person),+time))
%modeh(terminatedAt(moving(+person,+person),+time))

modeh(initiatedAt(meeting(+person,+person),+time))
modeh(terminatedAt(meeting(+person,+person),+time))

%modeh(initiatedAt(leavingObject(+person,+person),+time))
%modeh(terminatedAt(leavingObject(+person,+person),+time))

modeb(happensAt(walking(+person),+time))
modeb(happensAt(active(+person),+time))
modeb(happensAt(inactive(+person),+time))
modeb(happensAt(abrupt(+person),+time))
modeb(happensAt(running(+person),+time))
modeb(close(+person,+person,#threshold_value,+time))
modeb(far(+person,+person,#threshold_value,+time))
%modeb(holdsAt(visible(+person),+time))
modeb(happensAt(appear(+person),+time))
modeb(happensAt(disappear(+person),+time))

