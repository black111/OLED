modeh(initiatedAt(location(+person,+place),+time))
modeh(terminatedAt(location(+person,+place),+time))



modeb(happensAt(travel(+person,+place),+time))
modeb(happensAt(travel(+person,-place),+time))

modeb(happensAt(go_back(+person,+place),+time))
modeb(happensAt(go_back(+person,-place),+time))

modeb(happensAt(go(+person,+place),+time))
modeb(happensAt(go(+person,-place),+time))

modeb(happensAt(move(+person,+place),+time))
modeb(happensAt(move(+person,-place),+time))

modeb(happensAt(journey(+person,+place),+time))
modeb(happensAt(journey(+person,-place),+time))

modeb(neq(+place,+place))


examplePattern(holdsAt(location(+person,+place),+time))




